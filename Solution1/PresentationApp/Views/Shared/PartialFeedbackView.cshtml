@if (ViewData["feedback"] != null)
{
    <div class="alert alert-primary" role="alert">
        @ViewData["feedback"]
    </div>
}

@if (ViewData["warning"] != null)
{
    <div class="alert alert-danger" role="alert">
        @ViewData["warning"]
    </div>
}